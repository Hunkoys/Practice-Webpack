{
  "compilerOptions": {
    "outDir": "dist/",
    "baseUrl": "./",
    "paths": {
      "Utilities/*": ["src/utilities/*"],
      "Img/*": ["img/*"]
    },
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es2020",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
